				      // Ïù¥Ï†Ñ Îã¨Î°ú Ïù¥Îèô Î≤ÑÌäº ÏöîÏÜå ÏÑ†ÌÉù
				      const prevMonthButton = calendarElement.querySelector('.prev-month');
				      // Îã§Ïùå Îã¨Î°ú Ïù¥Îèô Î≤ÑÌäº ÏöîÏÜå ÏÑ†ÌÉù
				      const nextMonthButton = calendarElement.querySelector('.next-month');

				      // ÌòÑÏû¨ ÎÇ†ÏßúÎ•º Ï†ÄÏû•
				      let currentDate = new Date("${seldate}");
					  
				      // Îã¨Î†•ÏùÑ Î†åÎçîÎßÅÌïòÎäî Ìï®Ïàò
				      function renderCalendar(date) {
				          // ÌòÑÏû¨ Ïó∞ÎèÑÏôÄ ÏõîÏùÑ Í∞ÄÏ†∏Ïò¥
				          const year = date.getFullYear();
				          const month = date.getMonth();
				          // Ïù¥Î≤à Îã¨Ïùò Ï≤´ Î≤àÏß∏ ÎÇ†Ïùò ÏöîÏùºÏùÑ Í∞ÄÏ†∏Ïò¥ (0: ÏùºÏöîÏùº, 6: ÌÜ†ÏöîÏùº)
				          const firstDayOfMonth = new Date(year, month, 1).getDay();
				          // Ïù¥Î≤à Îã¨Ïùò ÎßàÏßÄÎßâ ÎÇ†ÏßúÎ•º Í∞ÄÏ†∏Ïò¥
				          const lastDateOfMonth = new Date(year, month + 1, 0).getDate();
				          // Ïù¥Ï†Ñ Îã¨Ïùò ÎßàÏßÄÎßâ ÎÇ†ÏßúÎ•º Í∞ÄÏ†∏Ïò¥
				          const lastDayOfPrevMonth = new Date(year, month, 0).getDate();

				          // Ìó§ÎçîÏóê ÎÖÑÎèÑÏôÄ Ïõî ÏÑ§Ï†ï
				          monthYearElement.textContent = year+"ÎÖÑ "+ (month+1)+ "Ïõî";

				          // Ïù¥Ï†ÑÏùò ÎÇ†Ïßú Î™©Î°ùÏùÑ Î™®Îëê ÏßÄÏõÄ
				          datesElement.innerHTML = '';

				          // Ïù¥Ï†Ñ Îã¨Ïùò ÎÇ†ÏßúÎ•º ÌëúÏãú
				          for (let i = firstDayOfMonth - 1; i >= 0; i--) {
				              const dayElement = document.createElement('div');
				              dayElement.classList.add('inactive'); // ÎπÑÌôúÏÑ±ÌôîÎêú ÎÇ†ÏßúÎ°ú ÌëúÏãú
				              dayElement.textContent = lastDayOfPrevMonth - i;
				              datesElement.appendChild(dayElement);
				          }

				          // Ïù¥Î≤à Îã¨Ïùò ÎÇ†ÏßúÎ•º ÌëúÏãú
				          for (let i = 1; i <= lastDateOfMonth; i++) {
				              const dayElement = document.createElement('div');
				              dayElement.textContent = i;
				              datesElement.appendChild(dayElement);
				          }

				          // Îã§Ïùå Îã¨Ïùò ÎÇ†ÏßúÎ•º ÌëúÏãú
				          const totalDates = firstDayOfMonth + lastDateOfMonth;
				          const nextMonthDates = (Math.ceil(totalDates / 7) * 7) - totalDates;

				          for (let i = 1; i <= nextMonthDates; i++) {
				              const dayElement = document.createElement('div');
				              dayElement.classList.add('inactive'); // ÎπÑÌôúÏÑ±ÌôîÎêú ÎÇ†ÏßúÎ°ú ÌëúÏãú
				              dayElement.textContent = i;
				              datesElement.appendChild(dayElement);
				          }
				      }

				      // Ïù¥Ï†Ñ Îã¨ Î≤ÑÌäº ÌÅ¥Î¶≠ Ïù¥Î≤§Ìä∏ Ï≤òÎ¶¨
				      prevMonthButton.addEventListener('click', function () {
				          currentDate.setMonth(currentDate.getMonth() - 1); // ÌòÑÏû¨ ÎÇ†ÏßúÎ•º Ïù¥Ï†Ñ Îã¨Î°ú ÏÑ§Ï†ï
						  year = currentDate.getFullYear();
  						  month = ("0"+(currentDate.getMonth()+1)).slice(-2);
  						  location = "report?seldate="+year+"-"+month+"-"+"01";
				          //renderCalendar(currentDate); // Îã¨Î†• Îã§Ïãú Î†åÎçîÎßÅ
				      });

				      // Îã§Ïùå Îã¨ Î≤ÑÌäº ÌÅ¥Î¶≠ Ïù¥Î≤§Ìä∏ Ï≤òÎ¶¨
				      nextMonthButton.addEventListener('click', function () {
						  currentDate.setMonth(currentDate.getMonth() + 1); // ÌòÑÏû¨ ÎÇ†ÏßúÎ•º Îã§Ïùå Îã¨Î°ú ÏÑ§Ï†ï
						  year = currentDate.getFullYear();
						  month = ("0"+(currentDate.getMonth()+1)).slice(-2);
						  location = "report?seldate="+year+"-"+month+"-"+"01";
				          //renderCalendar(currentDate); // Îã¨Î†• Îã§Ïãú Î†åÎçîÎßÅ
				      });

				      // Ï¥àÍ∏∞ Îã¨Î†• Î†åÎçîÎßÅ
				      renderCalendar(currentDate);
					  
					  // ÎÇ†Ïßú ÌÅ¥Î¶≠Ïãú ÏÑ†ÌÉù ÎÇ†Ïßú ÌéòÏù¥ÏßÄ
					  
	  				  $('.calendar-dates').on('click', 'div:not(.inactive)', function(){
							var a = $('.month-year').text().split(' ');
							var b = a[0].slice(0,-1)+"-"+("0"+a[1]).slice(-3).slice(0,-1);
	  						let seldate = b+"-"+("0"+$(this).text()).slice(-2);
							location = "report?seldate="+seldate;
	  				  });
					  
				  }); // end dom loaded Ïù¥Î≤§Ìä∏ 
				  
				// Îã¨Î†•Ïóê Ìï¥ÎãπÎÇ†Ïßú ÌëúÏãú (ÌÅ¥ÎûòÏä§Ï∂îÍ∞Ä + css)
				$(function(){
					let d = Number('${seldate}'.slice(-2));
				$('.calendar-dates div').filter(function() {
					 return $(this).text()==d && !$(this).hasClass('inactive');
					}).addClass('select-day');
				});
    </script>
</body>

